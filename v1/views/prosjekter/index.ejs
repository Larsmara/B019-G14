<% include ../partials/header %>

<main>
  <h2>Prosjekter</h2>
  <p>En oversikt over prosjektene. </p>
  <p>Totalt antall idéer: <%= project.length%></p>

  <% if(selected.length > null){ %>
  <h3>Til produksjon</h3>
  <% } %>
  <div>
    <% selected.forEach(function(selected){ %>
      <div class="">
        <a href="/prosjekter/<%=selected._id%>"><%= selected.title %></a>
      </div>
    <% }) %>
  </div>
  <hr>
  <br>
  
  <h3>Alle ideér</h3>
  <div>
    <% project.forEach(function(project){ %>
      <div>
        <a href="/prosjekter/<%=project._id%>"><%= project.title %></a>
      </div>
    <% }) %>
  </div>

</main>

<% include ../partials/footer %>
